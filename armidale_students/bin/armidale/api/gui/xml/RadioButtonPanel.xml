             type="int"
             default="VERTICAL"
             constants="armidale.api.gui.constants.Orientation"
  />
  
  <list name="RadioButton"
        type="@armidale.api.gui.RadioButton"
  />

  <include filename="SelectionCallback.xml"/>
   
</ArmidaleClass>
